# [1] E-COMMERCE WEBSITE (potensi SQLi: product detail, search, filter, pagination)
inurl:product.php?id=            # product detail page
inurl:item.php?product_id=       # product detail page
inurl:shop.php?cat_id=           # filter kategori
inurl:filter.php?sort_by=        # sort/filter produk
inurl:search.php?query=          # pencarian produk
inurl:category.php?tag=          # filter berdasarkan tag
inurl:page.php?page=             # pagination

# [2] CMS WEBSITE (potensi SQLi: page view, post, author)
inurl:view.php?page_id=          # view halaman dinamis
inurl:article.php?news_id=       # view artikel
inurl:read.php?article_id=       # baca artikel
inurl:author.php?author_id=      # filter berdasarkan penulis
inurl:blog.php?entry_id=         # entry blog

# [3] FORUM / KOMUNITAS (potensi SQLi: thread view, reply, user profile)
inurl:forum.php?thread_id=       # view thread forum
inurl:post.php?post_id=          # view post forum
inurl:comment.php?comment_id=    # lihat komentar
inurl:message.php?msg_id=        # private message
inurl:profile.php?user_id=       # profil user

# [4] SITUS BERITA / ARTIKEL (potensi SQLi: artikel, kategori, tag)
inurl:article.php?news_id=       # berita berdasarkan ID
inurl:category.php?tag=          # tag/kategori berita
inurl:read.php?article_id=       # baca artikel

# [5] APLIKASI INTERNAL / ADMIN PANEL (potensi SQLi: laporan, data internal, invoice)
inurl:detail.php?report_id=      # detail laporan
inurl:view.php?invoice_id=       # faktur/invoice
inurl:client.php?client_id=      # data klien
inurl:admin/view.php?record_id=  # record database (sering bypass auth)

# [6] APLIKASI SEKOLAH / KAMPUS (potensi SQLi: nilai, jadwal, hasil siswa)
inurl:result.php?student_id=     # hasil ujian siswa
inurl:grade.php?nilai_id=        # nilai siswa
inurl:schedule.php?jadwal_id=    # jadwal kuliah
inurl:course.php?course_id=      # informasi mata kuliah

# [7] SITUS DENGAN FILTERING / SEARCH (potensi SQLi: union-based/filter bypass)
inurl:filter.php?sort_by=        # filter data
inurl:search.php?query=          # pencarian umum
inurl:display.php?type_id=       # jenis tampilan
inurl:list.php?event_id=         # event/kegiatan
